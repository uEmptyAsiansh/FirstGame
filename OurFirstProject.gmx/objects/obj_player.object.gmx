          <argument>
            <kind>1</kind>
            <string>///get the player's input/collisions
//check if player is pressing right and left and move in that direction
key_right = keyboard_check (vk_right); 
key_left = -keyboard_check (vk_left);
key_jump = keyboard_check_pressed (vk_up); //only if you pressed the key

//react to inputs
//make horizontal move 1 or -1
move = key_left + key_right;
hsp = move * movespeed;

if(hsp!=0)
{
    
     sprite_index= v1Bun_running;
     draw_xscale= sign(hsp);
   
}
else
{
sprite_index = v1Bun_idle;
}

//turn vertical movement, have gravity, stretch
if (vsp &lt; 10) vsp += grav;

if (place_meeting(x,y+1,obj_floor_grass))
{
    vsp = key_jump * -jumpspeed;
}

if (keyboard_check_pressed(vk_up))
{
    draw_yscale=1.65;
    draw_xscale=.75;
}


//erase scale 
draw_xscale=lerp(draw_xscale, 1, .05);
draw_yscale=lerp(draw_yscale,1,.05);

//horizontal collision
if (place_meeting (x+hsp,y,obj_floor_grass))
{
    while(!place_meeting(x+sign(hsp),y,obj_floor_grass))  //sign return 1 or -1
    {
        x += sign(hsp);
        
    }
    hsp = 0;

}
x += hsp;



//vertical Collsion
if (place_meeting (x,y+vsp,obj_floor_grass))
{
    while(!place_meeting(x,y+sign(vsp),obj_floor_grass))  //sign return 1 or -1
    {
        y += sign(vsp);
    }
    vsp = 0;

}
y += vsp;

// Scale on landing
if (place_meeting(x, y+1, obj_floor_grass) &amp;&amp; !place_meeting(x,yprevious+1,obj_floor_grass))
{
    draw_yscale=.75;
    draw_xscale=1.45;
}



if (vsp &lt; 0)
{
    sprite_index = v1Bun_jumping;
}
else if (vsp &gt; 2)
{
    sprite_index = v1Bun_falling;
}

</string>
